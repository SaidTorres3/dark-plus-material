{
  "cSpell.words": [
    "Preformat",
    "Promisified",
    "alignas",
    "alignof",
    "enummember",
    "instanceof",
    "noexcept",
    "scrollbar",
    "uintptr",
    "vangware",
    "vendored",
    "wordlike"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.enable": true,
  "typescript.tsdk": "./node_modules/typescript/lib"
}
